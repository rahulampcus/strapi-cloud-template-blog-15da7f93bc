{
  "kind": "collectionType",
  "collectionName": "lake_visits",
  "info": {
    "singularName": "lake-visit",
    "pluralName": "lake-visits",
    "displayName": "lake-visit"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "visit_date": {
      "type": "datetime",
      "required": false
    },
    "officer_name": {
      "type": "string",
      "required": false
    },
    "visit_type": {
      "type": "enumeration",
      "required": false,
      "enum": [
        "Survey",
        "Stock Check",
        "Monitoring"
      ]
    },
    "startDate": {
      "type": "date"
    },
    "endDate": {
      "type": "date"
    },
    "visitStatus": {
      "type": "enumeration",
      "default": "NOT-STARTED",
      "enum": [
        "NOT-STARTED",
        "PENDING",
        "COMPLETED",
        "IN-PROGRESS",
        "IN-REVIEW",
        "OFFICE-REJECTED",
        "NEED-MORE-INFO",
        "READY-FOR-VISIT"
      ]
    },
    "officerRemark": {
      "type": "enumeration",
      "enum": [
        "APPROVED",
        "REJECTED",
        "NEED-MORE-INFO"
      ]
    },
    "officerRemarkInfo": {
      "type": "text"
    },
    "lake": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::lake.lake",
      "inversedBy": "lake_visits"
    },
    "visit_report": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::visit-report.visit-report",
      "inversedBy": "lake_visit"
    }
  }
}
