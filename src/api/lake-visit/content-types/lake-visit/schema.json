{
  "kind": "collectionType",
  "collectionName": "lake_visits",
  "info": {
    "singularName": "lake-visit",
    "pluralName": "lake-visits",
    "displayName": "lake-visit"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "visit_date": {
      "type": "datetime",
      "required": true
    },
    "officer_name": {
      "type": "string",
      "required": true
    },
    "visit_type": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "Survey",
        "Stock Check",
        "Monitoring"
      ]
    },
    "startDate": {
      "type": "date"
    },
    "endDate": {
      "type": "date"
    },
    "lake": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::lake.lake",
      "inversedBy": "lake_visits"
    },
    "visit_reports": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::visit-report.visit-report",
      "mappedBy": "lake_visit"
    }
  }
}
